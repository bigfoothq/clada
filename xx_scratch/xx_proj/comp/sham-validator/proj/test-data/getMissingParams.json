{
  "cases": [
    {
      "name": "no missing params",
      "input": [
        {"path": "/tmp/test.txt", "content": "hello"},
        ["path", "content"]
      ],
      "expected": []
    },
    {
      "name": "one missing param",
      "input": [
        {"path": "/tmp/test.txt"},
        ["path", "content"]
      ],
      "expected": ["content"]
    },
    {
      "name": "all params missing",
      "input": [
        {},
        ["path", "content"]
      ],
      "expected": ["path", "content"]
    },
    {
      "name": "null value counts as missing",
      "input": [
        {"path": "/tmp/test.txt", "content": null},
        ["path", "content"]
      ],
      "expected": ["content"]
    },
    {
      "name": "undefined counts as missing",
      "input": [
        {"path": "/tmp/test.txt", "content": undefined},
        ["path", "content"]
      ],
      "expected": ["content"]
    },
    {
      "name": "empty string is not missing",
      "input": [
        {"path": "/tmp/test.txt", "content": ""},
        ["path", "content"]
      ],
      "expected": []
    },
    {
      "name": "zero is not missing",
      "input": [
        {"count": 0, "offset": 10},
        ["count", "offset"]
      ],
      "expected": []
    },
    {
      "name": "false is not missing",
      "input": [
        {"recursive": false, "force": true},
        ["recursive", "force"]
      ],
      "expected": []
    },
    {
      "name": "no required params",
      "input": [
        {"optional": "value"},
        []
      ],
      "expected": []
    }
  ]
}